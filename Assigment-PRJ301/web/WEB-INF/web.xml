<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>controller.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListStudentController</servlet-name>
        <servlet-class>controller.ListStudentController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GroupController</servlet-name>
        <servlet-class>controller.GroupController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SujectController</servlet-name>
        <servlet-class>controller.SujectController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MarkController</servlet-name>
        <servlet-class>controller.MarkController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Mark2Controller</servlet-name>
        <servlet-class>controller.Mark2Controller</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutController</servlet-name>
        <servlet-class>controller.LogoutController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeController</servlet-name>
        <servlet-class>controller.HomeController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EnrollSujectController</servlet-name>
        <servlet-class>controller.EnrollSujectController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AssessmentController</servlet-name>
        <servlet-class>controller.AssessmentController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListStudentController</servlet-name>
        <url-pattern>/student/list</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GroupController</servlet-name>
        <url-pattern>/group</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SujectController</servlet-name>
        <url-pattern>/suject</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MarkController</servlet-name>
        <url-pattern>/markview</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Mark2Controller</servlet-name>
        <url-pattern>/mark</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutController</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeController</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EnrollSujectController</servlet-name>
        <url-pattern>/enrollsuject</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AssessmentController</servlet-name>
        <url-pattern>/assessment</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
